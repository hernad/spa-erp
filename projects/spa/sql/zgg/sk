  hr.restart.baza.Condition.whereAllEqual(new String[] {"KNJIG","CPAR","VRDOK","BROJDOK","VRDOK","BROJIZV"}, qds));
  result.setBigDecimal("OSNOVICA10",new BigDecimal(0));
  result.setBigDecimal("PDV10",new BigDecimal(0));
  result.setBigDecimal("OSNOVICA22",new BigDecimal(0));
  result.setBigDecimal("PDV22",new BigDecimal(0));
  result.setBigDecimal("NEOPOREZIVO",new BigDecimal(0));
  result.setBigDecimal("OSTALO",new BigDecimal(0));
  qdstavke.open();
  for (qdstavke.first(); qdstavke.inBounds(); qdstavke.next()) {
    if (qdstavke.getInt("RBS") == 1) continue;
    if (qdstavke.getShort("CKOLONE") == 7) {//O10%
      result.setBigDecimal("OSNOVICA10",result.getBigDecimal("OSNOVICA10")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 8) {//O22%
      result.setBigDecimal("OSNOVICA22",result.getBigDecimal("OSNOVICA22")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 11) {//P10%
      result.setBigDecimal("PDV10",result.getBigDecimal("PDV10")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 13) {//P22%
      result.setBigDecimal("PDV22",result.getBigDecimal("PDV22")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 511) {
      result.setBigDecimal("NEOPOREZIVO",result.getBigDecimal("NEOPOREZIVO")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else {
      result.setBigDecimal("OSTALO",result.getBigDecimal("OSTALO")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    }
    
  }
  result.setBigDecimal("RAZLIKA22",result.getBigDecimal("OSNOVICA22").multiply(new BigDecimal(0.22))
            .add(result.getBigDecimal("PDV22").negate()).setScale(2,BigDecimal.ROUND_HALF_UP));
  result.setBigDecimal("RAZLIKA10",result.getBigDecimal("OSNOVICA10").multiply(new BigDecimal(0.10))
            .add(result.getBigDecimal("PDV10").negate()).setScale(2,BigDecimal.ROUND_HALF_UP));
  result.post();
}


//POTVRDJENI!!
qry = "select * from skstavke WHERE SKSTAVKE.CPAR = "+acpar+" AND SKSTAVKE.VRDOK IN ('URN','OKD') AND (SKSTAVKE.DATPRI BETWEEN '"+_dpod+" 00:00:00' AND '"+_dpdo+" 23:59:59') AND SKSTAVKE.KNJIG = '"+knjig+"'";
qds = hr.restart.util.Util.getNewQueryDataSet(qry);
for (qds.first(); qds.inBounds(); qds.next()) {
  result.insertRow(false);
  pp = hr.restart.util.Util.getNewQueryDataSet(
    "SELECT NAZPAR FROM partneri where cpar="+qds.getInt("CPAR")
  );
  pp.open();

  result.setString("PARTNER",pp.getString("NAZPAR"));
  result.setString("BROJDOK",qds.getString("BROJDOK"));
  result.setTimestamp("DATUM",qds.getTimestamp("DATDOK"));

  result.setString("CKNJIGE",qds.getString("CKNJIGE")+" / "+qds.getString("VRDOK"));  
  result.setInt("UFA",Integer.parseInt(qds.getString("EXTBRDOK").trim()));
  result.setBigDecimal("IZNOS",qds.getBigDecimal("ID").add(qds.getBigDecimal("IP")));
  qdstavke = hr.restart.gk.frmKnjSKRac.getUIStavke(qds);
  result.setBigDecimal("OSNOVICA10",new BigDecimal(0));
  result.setBigDecimal("PDV10",new BigDecimal(0));
  result.setBigDecimal("OSNOVICA22",new BigDecimal(0));
  result.setBigDecimal("PDV22",new BigDecimal(0));
  result.setBigDecimal("NEOPOREZIVO",new BigDecimal(0));
  result.setBigDecimal("OSTALO",new BigDecimal(0));
  qdstavke.open();
  for (qdstavke.first(); qdstavke.inBounds(); qdstavke.next()) {
    if (qdstavke.getInt("RBS") == 1) continue;
    if (qdstavke.getShort("CKOLONE") == 7) {//O10%
      result.setBigDecimal("OSNOVICA10",result.getBigDecimal("OSNOVICA10")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 8) {//O22%
      result.setBigDecimal("OSNOVICA22",result.getBigDecimal("OSNOVICA22")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 11) {//P10%
      result.setBigDecimal("PDV10",result.getBigDecimal("PDV10")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 13) {//P22%
      result.setBigDecimal("PDV22",result.getBigDecimal("PDV22")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else if (qdstavke.getShort("CKOLONE") == 511) {
      result.setBigDecimal("NEOPOREZIVO",result.getBigDecimal("NEOPOREZIVO")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    } else {
      result.setBigDecimal("OSTALO",result.getBigDecimal("OSTALO")
        .add(qdstavke.getBigDecimal("ID").add(qdstavke.getBigDecimal("IP"))));
    }
    
  }
  result.setBigDecimal("RAZLIKA22",result.getBigDecimal("OSNOVICA22").multiply(new BigDecimal(0.22))
            .add(result.getBigDecimal("PDV22").negate()).setScale(2,BigDecimal.ROUND_HALF_UP));
  result.setBigDecimal("RAZLIKA10",result.getBigDecimal("OSNOVICA10").multiply(new BigDecimal(0.10))
            .add(result.getBigDecimal("PDV10").negate()).setScale(2,BigDecimal.ROUND_HALF_UP));
  result.post();
}

result;

